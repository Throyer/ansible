[wordpress]
192.168.1.13 ansible_user=vagrant ansible_ssh_private_key_file=".vagrant/machines/wordpress/virtualbox/private_key"

[wordpress:vars]
ansible_ssh_common_args='-o StrictHostKeyChecking=no'

